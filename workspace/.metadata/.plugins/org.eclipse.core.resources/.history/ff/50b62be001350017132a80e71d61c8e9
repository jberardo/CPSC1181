package question01;

public class TrimIt {

	public static void main(String[] args) {
		String sentence1 = " abc ";
		String sentence2 = " def";
		String sentence3 = "ghi ";
		
		String message = "";
		
		message = sentence1.trim().concat(sentence2.trim().contat(sentence3.trim().=));
		
		System.out.println(message);
	}
}
