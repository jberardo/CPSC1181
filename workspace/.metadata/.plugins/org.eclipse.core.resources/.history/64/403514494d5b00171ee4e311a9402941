package question01;

public class Billfold {
	Card card1;
	Card card2;
	
	public BillFold()
	{
		
	}
	
	public void addCard(Card card)
	{
		if (card1 == null)
		{
			card1 = new Card();
		}
		else if (card2 == null)
		{
			card2 = new Card();
		}
	}
	
	public String formatCards()
	{
		return "[" + card1.format() + "|" + card2.format() + "]";
	}
}
