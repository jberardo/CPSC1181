package question01;

public class Card
{
	private String name;

	public Card()
	{
		name = "";
	}
	
	public Card(String n)
	{
		name = n;
	}
	
	public String getName()
	{
		return name;
	}
	
	public void setName(String name)
	{
		this.name = name;
	}
	
	public boolean isExpired()
	{
		return false;
	}
	
	public String format()
	{
		return "Card holder: " + name;
	}
	
	public String toString()
	{
		return "Card [Name=" + this.name + "]";
	}
}
