package question03;

/**
 * 
 * @author Joao Berardo
 * @since 1.0, June, 01 2017
 *
 */
public class Cat implements Speakable {
	private String name;

	public Cat(String name)
	{
		this.name = name;
	}

	public void speak()
	{
		System.out.println("Meow! Meow!");
	}

	public String toString()
	{
		return "Cat: " + name;
	}
}