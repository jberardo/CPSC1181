package book;

import common.Picture;

public class PictureDemo
{
	public static void main(String[] args)
	{
		Picture pic = new Picture();
		pic.load("skull.jpeg");
        int newWidth = pic.getWidth() - 40;
	    int newHeight = pic.getHeight() - 40;
	    pic.scale(newWidth, newHeight);
	    pic.move(20, 20);
		pic.border(5);
		
	}
}