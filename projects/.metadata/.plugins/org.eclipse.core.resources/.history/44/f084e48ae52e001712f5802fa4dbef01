package chapter2.exercises;

import java.awt.Rectangle;

// constructs a Rectangle object and then computes
// and prints its area. Use the getWidth and getHeight methods. Also print the expected
// answer.

public class AreaTester {
	private int width = 0;
	private int heigth = 0;
	
	public AreaTester(int w, int h) {
		this.width = w;
		this.heigth = h;
	}
	
	Rectangle rec = new Rectangle(5, 10, this.width, this.heigth);
	
	public void setWidth() {
		
	}
	
	public void setHeigth(int h) {
		heigth = h;
	}
	
	public double getArea() {
		return rec.getWidth() * rec.getHeight();
	}
}
