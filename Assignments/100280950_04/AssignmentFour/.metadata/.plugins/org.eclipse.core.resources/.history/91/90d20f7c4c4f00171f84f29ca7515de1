import java.util.Scanner;

/**
 *	<h3>DataSetTester</h3>
 *	Program to test DataSet class<br/>
 *
 * 
 * @author Joao Berardo
 * @version 1.0
 * @since June, 12 2017
 *
 */
public class DataSetTester {
	/**
	 * 
	 * @param args
	 */
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		DataSet data = new DataSet();
		
		data.add(1);
		data.add(1);
		data.add(2);
		data.add(1);
		data.add(1);
		
		System.out.println("Count: " + data.getCount() + ", Expected: 5");
		System.out.println("Sum: " + data.getSum() + ", Expected: 6.0");
		System.out.println("Average: " + data.getAverage() + ", Expected: 1.2");
		System.out.println("Standard Deviation: " + data.getStandardDeviation() + ", Expected: 2.48");
	}
}