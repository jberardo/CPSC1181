import java.time.Instant;
import java.util.Date;

public class Appointment
{
	private String description;
	private Date date;
	
	public Appointment()
	{
		this.description = "";
		this.date = new Date();
	}
	
	public Appointment(String newDescription, Date newDate)
	{
		this.description = newDescription;
		this.date = newDate;
	}
}
