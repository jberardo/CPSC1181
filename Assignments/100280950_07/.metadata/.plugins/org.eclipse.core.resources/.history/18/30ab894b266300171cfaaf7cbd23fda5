public class Clock
{
	/**
	 * 
	 */
	public Clock()
	{
		
	}
	
	/**
	 * 
	 */
	public String getHours()
	{
		String date = java.time.Instant.now().toString();
		System.out.println(date);
		return "";
	}
	
	public String getMinutes()
	{
		// java.time.Instant.now().toString()
		return "";
	}
	
	public String getTime()
	{
		return this.getHours() + ":" + this.getMinutes();
	}
}